{
  "name": "gp-pretty-print",
  "version": "0.0.2",
  "description": "Build a representation of source code and emit as text",
  "main": "dist/index-umd.js",
  "scripts": {
    "eslint": "eslint .",
    "rollup": "rollup -c",
    "test": "npm run eslint && npm run rollup && mocha --require @babel/register",
    "start": "static -p 8055",
    "build-docs": "rm -rf docs/jsdoc/* && jsdoc --pedantic -c docs/jsdoc-config.js .",
    "open-docs": "opn http://localhost:8055/docs/jsdoc/ && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gentleman-programmer/gp-pretty-print.git"
  },
  "keywords": [
    "format",
    "text",
    "source-code"
  ],
  "author": "Kevin Lindsey",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/gentleman-programmer/gp-pretty-print/issues"
  },
  "homepage": "https://github.com/gentleman-programmer/gp-pretty-print#readme",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "@mysticatea/eslint-plugin": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-config-ash-nazg": "^4.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsdoc": "^4.8.3",
    "eslint-plugin-markdown": "^1.0.1",
    "eslint-plugin-no-use-extend-native": "^0.4.1",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^8.0.2",
    "esm": "^3.2.22",
    "jsdoc": "^3.6.3",
    "mocha": "^6.2.2",
    "node-static": "^0.7.11",
    "opn-cli": "^4.1.0",
    "rollup": "^1.26.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-terser": "^4.0.4",
    "typescript": "^3.6.4"
  },
  "engines": {
    "node": ">= 10.15.3"
  }
}
